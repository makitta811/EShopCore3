<body class="js">

    <script type="text/javascript">

        function ConvertList() {
            $.ajax({
                async: true,
                dataType: 'HTML',
                type: 'GET',
                contentType: false,
                processData: false,
                url: '/Home/ListView',
                success: function (data) {
                    document.getElementById("init").innerHTML = data;
                },
                error: function () {
                    alert("Ne go risuva!")
                }
            });
        }

        function ConvertGrid() {
            $.ajax({
                async: true,
                dataType: 'HTML',
                type: 'GET',
                contentType: false,
                processData: false,

                url: '/Home/GridView',
                success: function (data) {
                    document.getElementById("init").innerHTML = data;
                },
                error: function () {
                    alert("Ne go risuva!")
                }
            });
        }

        function AddToCart(item) {
            var itemId = $(item).attr("itemId");
            var formData = new FormData;
            formData.append("itemId", itemId);

            $.ajax({
                async: true,
                dataType: 'JSON',
                type: 'POST',
                contentType: false,
                processData: false,
                data: formData,
                url: '/Home/Index',
                success: function (data) {
                    if (data.success) {
                        alert(data.message);

                        $.ajax({
                            async: true,
                            dataType: 'HTML',
                            type: 'GET',
                            contentType: false,
                            processData: false,
                            data: formData,
                            url: '/Home/GetCart',
                            success: function (data) {
                                document.getElementById("sinlge-bar shopping").innerHTML = data;
                            },
                            error: function () {
                                alert("Ne go risuva!")
                            }
                        });
                    }
                },
                error: function () {
                    alert("Something went wrong!");
                }
            });
        }
    </script>

